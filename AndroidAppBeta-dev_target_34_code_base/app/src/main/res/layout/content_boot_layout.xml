                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left"
                    android:fontFamily="@font/sf_pro_display_regular"
                    android:text="@string/scr_lbl_remarks"
                    android:textColor="@color/light_grey_100"
                    android:textSize="@dimen/_14sp" />

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/textInputLayoutVehRemark"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense.ExposedDropdownMenu"
                    android:layout_width="match_parent"
                    android:layout_height="48dp"
                    android:layout_marginTop="@dimen/_7dp"
                    app:boxCornerRadiusBottomEnd="@dimen/_10dp"
                    app:boxCornerRadiusBottomStart="@dimen/_10dp"
                    app:boxCornerRadiusTopEnd="@dimen/_10dp"
                    app:boxCornerRadiusTopStart="@dimen/_10dp"
                    app:hintEnabled="false"
                    app:hintTextAppearance="@style/Editext.Theme"
                    app:placeholderTextAppearance="@style/Editext.Theme">

                    <androidx.appcompat.widget.AppCompatAutoCompleteTextView
                        android:id="@+id/AutoComTextViewVehRemark"
                        android:layout_width="match_parent"
                        android:layout_height="48dp"
                        android:background="@drawable/round_corner_shape_without_fill_thin_blue"
                        android:clickable="true"
                        android:drawableRight="@drawable/ic_dropdown_arrow_blue"
                        android:editable="true"
                        android:ellipsize="start"
                        android:focusable="true"
                        android:focusableInTouchMode="true"
                        android:fontFamily="@font/poppins_regular"
                        android:hint="Select"
                        android:imeOptions="actionDone"
                        android:inputType="textAutoComplete"
                        android:maxLines="3"
                        android:paddingVertical="@dimen/_1dp"
                        android:textColor="@color/black_heading"
                        android:textColorHint="@color/black_heading"
                        android:textSize="@dimen/_12sp"
                        android:textAllCaps="true"
                        />
                </com.google.android.material.textfield.TextInputLayout>

            </androidx.appcompat.widget.LinearLayoutCompat>

        <androidx.appcompat.widget.LinearLayoutCompat
            android:id="@+id/ll_comment"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_8dp"
            android:layout_marginHorizontal="@dimen/_2dp"
            android:orientation="vertical">

            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="left"
                android:fontFamily="@font/sf_pro_display_regular"
                android:text="@string/scr_lbl_comments"
                android:textColor="@color/light_grey_100"
                android:textSize="@dimen/_14sp" />

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/input_textNote"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_7dp"
                android:layout_marginBottom="@dimen/_10dp"
                android:imeOptions="actionDone"
                app:boxCornerRadiusBottomEnd="@dimen/_10dp"
                app:boxCornerRadiusBottomStart="@dimen/_10dp"
                app:boxCornerRadiusTopEnd="@dimen/_10dp"
                app:boxCornerRadiusTopStart="@dimen/_10dp"
                app:hintTextAppearance="@style/Editext.Theme"
                app:placeholderTextAppearance="@style/Editext.Theme">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/etNote"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_120dp"
                    android:layout_marginTop="@dimen/_6dp"
                    android:background="@drawable/round_corner_shape_without_fill_thin_yellow"
                    android:gravity="start"
                    android:imeOptions="actionDone"
                    android:inputType="textMultiLine"
                    android:overScrollMode="always"
                    android:scrollbarStyle="insideInset"
                    android:scrollbars="vertical"
                    android:singleLine="false"
                    android:textAlignment="textStart"
                    android:textSize="@dimen/_15sp"
                    app:errorEnabled="true" />

            </com.google.android.material.textfield.TextInputLayout>

        </androidx.appcompat.widget.LinearLayoutCompat>

            <androidx.appcompat.widget.LinearLayoutCompat
                android:id="@+id/ll_checkbox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_16dp"
                android:layout_marginHorizontal="@dimen/_2dp"
                android:orientation="horizontal"
                android:weightSum="3">

                <androidx.appcompat.widget.AppCompatCheckBox
                    android:id="@+id/check_regular"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:text="@string/scr_lbl_regular"/>

                <androidx.appcompat.widget.AppCompatCheckBox
                    android:id="@+id/check_heavy"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:text="@string/scr_lbl_heavy"/>


                <androidx.appcompat.widget.AppCompatCheckBox
                    android:id="@+id/check_medium"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:text="@string/scr_lbl_medium"/>

            </androidx.appcompat.widget.LinearLayoutCompat>

            <androidx.appcompat.widget.LinearLayoutCompat
                android:id="@+id/lay_button_submit"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:weightSum="2"
                android:layout_marginTop="@dimen/_16dp"
                android:layout_marginBottom="@dimen/_26dp"
                android:orientation="horizontal">

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/btn_submit"
                    style="@style/Button_shape_blue"
                    android:onClick="onClick"
                    android:layout_weight="1"
                    android:padding="@dimen/_10dp"
                    android:text="@string/scr_btn_save_small_letter"
                    android:textAllCaps="false" />

                <View
                    android:id="@+id/viewPrintDivider"
                    android:layout_width="@dimen/_10dp"
                    android:layout_height="@dimen/_0dp"/>

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/btn_submit_issue"
                    android:layout_weight="1"
                    style="@style/Button_shape_without_fill"
                    android:layout_alignParentBottom="true"
                    android:padding="@dimen/_10dp"
                    android:text="@string/scr_btn_save_cite_small_letter"
                    android:textAllCaps="false" />

            </androidx.appcompat.widget.LinearLayoutCompat>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btn_submit_time"
                android:layout_weight="1"
                style="@style/Button_shape_without_fill"
                android:layout_alignParentBottom="true"
                android:padding="@dimen/_4dp"
                android:text="@string/scr_btn_save_time_small_letter"
                android:textAllCaps="false" />

            <View
                android:id="@+id/bottomView"
                android:layout_height="@dimen/_14dp"
                android:layout_width="match_parent"/>

        </androidx.appcompat.widget.LinearLayoutCompat>

