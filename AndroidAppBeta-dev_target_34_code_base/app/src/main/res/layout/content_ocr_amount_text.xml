<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout  xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginLeft="233dp"
    android:layout_marginTop="@dimen/_28dp"
    android:orientation="vertical"
    android:visibility="visible">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/appcomptext_sideId_1_1"
        style="@style/TextView_print_staring_field"
        android:layout_marginTop="-2dp"
        android:text="0" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/appcomptext_sideId_1_2"
        style="@style/TextView_print"
        android:layout_below="@+id/appcomptext_sideId_1_1"
        android:text="0" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/appcomptext_sideId_1_3"
        style="@style/TextView_print"
        android:layout_below="@+id/appcomptext_sideId_1_2"
        android:text="0" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/appcomptext_sideId_1_4"
        style="@style/TextView_print"
        android:layout_below="@+id/appcomptext_sideId_1_3"
        android:text="0" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/appcomptext_sideId_1_5"
        style="@style/TextView_print"
        android:layout_below="@+id/appcomptext_sideId_1_4"
        android:text="0" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/appcomptext_sideId_1_6"
        style="@style/TextView_print"
        android:layout_below="@+id/appcomptext_sideId_1_5"
        android:text="0" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/appcomptext_sideId_1_7"
        style="@style/TextView_print"
        android:layout_below="@+id/appcomptext_sideId_1_6"
        android:text="0"
        android:visibility="gone"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/appcomptext_sideId_1_8"
        style="@style/TextView_print"
        android:layout_below="@+id/appcomptext_sideId_1_7"
        android:text="0"
        android:visibility="gone"/>

</RelativeLayout>