<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar.Bridge">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/deep_blue</item>
        <item name="colorPrimaryDark">@color/deep_blue</item>
        <item name="colorAccent">@color/deep_blue</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowDisablePreview">true</item>//TODO - added for blank screen


        <!-- Logo shown in center -->
        <item name="windowSplashScreenAnimatedIcon">@mipmap/ic_launcher</item>

        <!-- Background color -->
        <item name="windowSplashScreenBackground">@color/ic_launcher_background</item>

        <!-- (Optional) branding image at bottom -->
        <!--        <item name="windowSplashScreenBrandingImage">@drawable/my_brand</item>-->

        <!-- Fallback if system splash not supported -->
        <!--        <item name="postSplashScreenTheme">@style/Theme.MyApp</item>-->

    </style>

    <style name="AppThemeGrey" parent="Theme.MaterialComponents.Light.NoActionBar.Bridge">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/gray</item>
        <item name="colorPrimaryDark">@color/light_grey</item>
        <item name="colorAccent">@color/light_grey</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowDisablePreview">true</item>//TODO - added for blank screen
    </style>

    <style name="Editext.Theme" parent="TextAppearance.AppCompat">
        <item name="android:textSize">@dimen/_12sp</item>
        <item name="colorAccent">@color/light_grey</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/light_grey</item>
        <item name="android:fontFamily">@font/poppins_bold</item>
        <item name="colorPrimary">@color/light_grey</item>
        <item name="colorPrimaryDark">@color/light_grey</item>
    </style>
    <style name="EditextTheme" parent="TextAppearance.AppCompat">
        <item name="android:textSize">@dimen/_12sp</item>
        <item name="colorAccent">@color/light_grey</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/light_grey</item>
        <item name="android:fontFamily">@font/poppins_bold</item>
        <item name="colorPrimary">@color/light_grey</item>
        <item name="colorPrimaryDark">@color/light_grey</item>
    </style>
    <!-- Style For Custom Dialog -->
    <style name="ThemeDialogCustom">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>

    <style name="AppTheme.White" parent="AppTheme">
        <!--Required Api 21 above-->
        <item name="android:windowLightStatusBar">true</item>
        <item name="colorPrimary">@color/deep_blue</item>
        <item name="colorPrimaryDark">@color/deep_blue</item>
        <!--  <item name="android:windowBackground">@drawable/get_started_theme</item>-->
